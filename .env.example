# Default session configuration
DEFAULT_LANGUAGE_CODE=gu-IN
DEFAULT_PROVIDER=groq
DEFAULT_SPEAKER=shubh

# Voice Backend WebSocket URL
VOICE_SERVER_URL=wss://voice-ind.onrender.com/

# For local development with local backend
# VOICE_SERVER_URL=ws://localhost:8081/

# ================================================
# GOOGLE CALENDAR (for demo booking)
# ================================================
# Service Account Email from Google Cloud Console
GOOGLE_SERVICE_ACCOUNT_EMAIL=

# Service Account Private Key (keep \n for newlines)
# Example: "-----BEGIN PRIVATE KEY-----\nMIIE...\n-----END PRIVATE KEY-----\n"
GOOGLE_SERVICE_ACCOUNT_PRIVATE_KEY=

# IMPORTANT: Use service account email as calendar ID, NOT "primary"
# "primary" refers to the service account's own empty calendar
GOOGLE_CALENDAR_ID=
